          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  u.full_name as \"Radnik\",\n  COUNT(DISTINCT ta.task_id) as \"Broj Zadataka\",\n  SUM(CASE \n    WHEN ta.task_type = 'SHIPPING' THEN COALESCE((\n      SELECT SUM(sol.picked_qty) \n      FROM shipping_order_lines sol \n      WHERE sol.\"orderId\" = ta.task_id\n    ), 0)\n    WHEN ta.task_type = 'RECEIVING' THEN COALESCE((\n      SELECT SUM(ri.received_quantity) \n      FROM receiving_items ri \n      WHERE ri.receiving_document_id = ta.task_id\n    ), 0)\n    ELSE 0 \n  END) as \"Ukupno Artikala\"\nFROM task_assignees ta\nJOIN users u ON u.id = ta.user_id\nWHERE ta.status = 'DONE'\n  AND ta.completed_at >= $__timeFrom()\n  AND ta.completed_at < $__timeTo()\nGROUP BY u.full_name\nORDER BY \"Ukupno Artikala\" DESC\nLIMIT 20;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
